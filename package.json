{
  "name": "document-outline-gen",
  "version": "1.0.0",
  "description": "Universal document outline structure generator for various file types and programming languages",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "outline",
    "table-of-contents",
    "schema",
    "ast",
    "markdown",
    "json",
    "xml",
    "yaml",
    "code-analysis",
    "document-structure"
  ],
  "author": "Alex Sabaka",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.23.9",
    "@babel/traverse": "^7.23.9",
    "acorn": "^8.11.3",
    "acorn-typescript": "^1.4.13",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^14.0.0",
    "fast-xml-parser": "^4.3.4",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "json-schema-library": "^10.1.2",
    "marked": "^12.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.23.9",
    "@types/babel__core": "^7.20.5",
    "@types/babel__parser": "^7.1.5",
    "@types/babel__traverse": "^7.20.5",
    "@types/cheerio": "^0.22.35",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
